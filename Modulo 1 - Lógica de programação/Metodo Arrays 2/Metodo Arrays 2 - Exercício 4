const professores = [
  { nome: 'teste', stack: 'backend' },
  { nome: 'teste1', stack: 'frontend' },
  { nome: 'teste3', stack: 'fullstak' },
  { nome: 'teste4', stack: 'backend' }
];

const filtroProfessoresBackend = professores.filter((professor) => {
  return professor.stack === 'backend';
});

console.log(filtroProfessoresBackend);

const filtroProfessoresFrontend = professores.filter((professor) => {
  return professor.stack === 'frontend';
});

console.log(filtroProfessoresFrontend);


