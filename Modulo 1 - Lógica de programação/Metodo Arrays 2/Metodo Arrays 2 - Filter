const numeros = [1, 4, 2, 5, 3];

const verificandoNumeros = numeros.filter((numero) => {
  return numero === 10;
});

console.log(verificandoNumeros);

console.log(numeros);

const nomes = ['teste', 'teste1', 'teste2', 'teste2'];

const verificandoNomes = nomes.filter((nome) => {
  return nome === 'teste2';
});

console.log(verificandoNomes);