// permite chamar uma função dentro de um método/ outra função, estipulando o tempo desejado para que a mesma rode.

function imprimirTeste() {
  console.log('Imprima este teste');
}
setTimeout(imprimirTeste, 2000);


setTimeout(function () {
  console.log('Imprima este teste 2')
}, 4000);


setTimeout(() => {
  console.log('Imprima este teste 3')
}, 10000);

