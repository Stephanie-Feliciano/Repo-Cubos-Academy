const usuarios = [
  { id: 11, nome: 'Teste', idade: 23 },
  { id: 7, nome: 'Teste1', idade: 28 },
  { id: 25, nome: 'Teste2', idade: 17 },
  { id: 1, nome: 'Teste3', idade: 32 },
  { id: 15, nome: 'Teste4', idade: 15 }
];

const ordenarId = usuarios.sort((a, b) => {
  return a.id - b.id;
});

console.log(ordenarId);

const novoOrdenarId = usuarios.sort((a, b) => {
  return b.id - a.id;
});

console.log(novoOrdenarId);