function imprimirApresentacao(pessoa) {
    console.log(`Olá! Meu nome é ${cliente.nome}, sou um jovem de ${cliente.idade} anos, ${cliente.altura} de altura e sou ${cliente.profissao}.`)
}

imprimirApresentacao(cliente);