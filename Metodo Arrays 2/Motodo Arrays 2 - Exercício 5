const produtos = [
  { nome: 'arroz', preco: 500 },
  { nome: 'feijão', preco: 900 },
  { nome: 'macarrão', preco: 300 },
  { nome: 'batata frita', preco: 1200 },
]

const valorComDesconto = produtos.map((produto) => {
  return {
    nome: produto.nome,
    preco: produto.preco,
    desconto: produto.preco * (10 / 100)
  }
});

console.log(valorComDesconto);